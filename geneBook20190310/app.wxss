/**app.wxss**/
/*初始化样式*/
view, text, navigator, picker, input, button, image, cover-image {
  display: flex;
  box-sizing: border-box;
}
page{
  background-color: #f4f8fb;
}
.flexcolumn {
  flex-direction: column;
}
.mask{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;

  background-color: rgba(0,0,0,0.5);
}

/*底线*/

.baseline {
  width: 100%;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  color: #999;
  padding-bottom: 20rpx;
  transition: 1s;
}

.ico_baseline {
  width: 262rpx;
  height: 86rpx;
  margin-bottom: 10rpx;
}
